@model IEnumerable<EntityFrameworkDD01.Models.Student>
@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>
<table border="2">
    <tr>
        <th> Id</th>
        <th>Name </th>
        <th>Gender </th>
        <th>Age</th>
    </tr>

    @{
        foreach (var item in Model)
        {
            <tr>

            <td>@item.Id</td>
            <td>@item.Name </td>
            <td>@item.Gender </td>
            <td>@item.Age</td>

            </tr>
        }
    }

</table>
